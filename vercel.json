{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "distDir": "dist",
                "runtime": "python3.6",
                "wsgiApplicationName": "application"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "dest": "/wsgi.py"
        }
    ]
}