{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/wsgi.py"
        }
    ]
}